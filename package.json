{
  "private": true,
  "name": "@adamelliotfields/gh-pages-action-example",
  "version": "1.0.0",
  "main": "src/index.html",
  "scripts": {
    "build": "run-s clean bundle",
    "bundle": "cross-env NODE_ENV=production parcel build --public-url=/gh-pages-action/ src/index.html",
    "clean": "rimraf dist",
    "deploy": "gh-pages -d dist",
    "start": "parcel src/index.html"
  },
  "devDependencies": {
    "bootstrap": "^4.4.1",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "jquery": "^3.4.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "popper.js": "^1.16.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  }
}
